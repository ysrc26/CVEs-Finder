{
  "manifest_version": 2,
  "name": "CVE Finder",
  "version": "1.0",
  "description": "Find CVEs easily by product name, vendor, and the specific version",
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "https://nvd.nist.gov/*"
  ]
}